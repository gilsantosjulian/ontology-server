<rdf:RDF 
	xmlns="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#" 
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
	xmlns:owl="http://www.w3.org/2002/07/owl#" 
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#" 
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xml:base="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology" 
	xmlns:dc="http://purl.org/dc/elements/1.1/" 
	xmlns:element="https://www.w3schools.com/tags/">
	<!--
        ///////////////////////////////////////////////////////////////////////////////////////
        //
        // OWL Header 
        //
        ///////////////////////////////////////////////////////////////////////////////////////
    -->
	<owl:Ontology rdf:about="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology">
		<dc:title>HTML Ontology</dc:title>
		<dc:description>Ontología que define los elementos HTML como componentes Web</dc:description>
	</owl:Ontology>
	<!--
        ///////////////////////////////////////////////////////////////////////////////////////
        //
        // Data properties
        //
        ///////////////////////////////////////////////////////////////////////////////////////
    -->
	<owl:DatatypeProperty rdf:about="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#id">
		<rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#general"></rdfs:subPropertyOf>
		<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"></rdfs:range>
	</owl:DatatypeProperty>

	<owl:DatatypeProperty rdf:about="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#role">
		<rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#aria"></rdfs:subPropertyOf>
		<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"></rdfs:range>
	</owl:DatatypeProperty>

	<owl:DatatypeProperty rdf:about="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#rel">
		<rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#aria"></rdfs:subPropertyOf>
		<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"></rdfs:range>
	</owl:DatatypeProperty>

	<owl:DatatypeProperty rdf:about="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#aria-controls">
		<rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#aria"></rdfs:subPropertyOf>
		<rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#properties"></rdfs:subPropertyOf>
		<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"></rdfs:range>
	</owl:DatatypeProperty>

	<owl:DatatypeProperty rdf:about="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#aria-labelledby">
		<rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#aria"></rdfs:subPropertyOf>
		<rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#properties"></rdfs:subPropertyOf>
		<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"></rdfs:range>
	</owl:DatatypeProperty>

	<owl:DatatypeProperty rdf:about="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#aria-expanded">
		<rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#aria"></rdfs:subPropertyOf>
		<rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#properties"></rdfs:subPropertyOf>
		<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"></rdfs:range>
	</owl:DatatypeProperty>

	<owl:DatatypeProperty rdf:about="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#aria-hidden">
		<rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#aria"></rdfs:subPropertyOf>
		<rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#properties"></rdfs:subPropertyOf>
		<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"></rdfs:range>
	</owl:DatatypeProperty>

	<owl:DatatypeProperty rdf:about="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#aria-haspopup">
		<rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#aria"></rdfs:subPropertyOf>
		<rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#properties"></rdfs:subPropertyOf>
		<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"></rdfs:range>
	</owl:DatatypeProperty>

	<owl:DatatypeProperty rdf:about="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#type">
		<rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#dublin-core"></rdfs:subPropertyOf>
		<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"></rdfs:range>
	</owl:DatatypeProperty>


	<!--
		///////////////////////////////////////////////////////////////////////////////////////
		//
		// Object Properties
		//
		///////////////////////////////////////////////////////////////////////////////////////
	-->
	<!--
		///////////////////////////////////////////////////////////////////////////////////////
		//
		// Classes
		//
		///////////////////////////////////////////////////////////////////////////////////////
	-->
	<owl:Class rdf:about="https://www.w3schools.com/html/html_elements.asp">
		<rdfs:label>Elemento HTML</rdfs:label>
		<rdfs:comment>Un elemento HTML usualmente consiste de un tag inicial y un tag final, con un contenido insertado en medio</rdfs:comment>
	</owl:Class>

	<owl:Class rdf:about="https://www.w3schools.com/tags/tag_header.asp">
		<rdfs:label>Encabezado</rdfs:label>
		<rdfs:comment>Representa el contenedor como contenido introductorio o como un conjunto de enlaces de navegación</rdfs:comment>
		<rdfs:subClassOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#Element"></rdfs:subClassOf>
	</owl:Class>

	<owl:Class rdf:about="https://www.w3schools.com/tags/tag_nav.asp">
		<rdfs:label>Barra de navegación</rdfs:label>
		<rdfs:comment>Define el conjunto de enlaces de navegación</rdfs:comment>
		<rdfs:subClassOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#Element"></rdfs:subClassOf>
	</owl:Class>

	<owl:Class rdf:about="https://www.w3schools.com/tags/tag_ul.asp">
		<rdfs:label>Lista desordenada</rdfs:label>
		<rdfs:comment>Define una lista desordenada con viñetas</rdfs:comment>
		<rdfs:subClassOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#Element"></rdfs:subClassOf>
		<rdfs:subClassOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#Nav"></rdfs:subClassOf>
	</owl:Class>

	<owl:Class rdf:about="https://www.w3schools.com/tags/tag_li.asp">
		<rdfs:label>Define un ítem de lista</rdfs:label>
		<rdfs:comment>Define el conjunto de enlaces de navegación</rdfs:comment>
		<rdfs:subClassOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#Element"></rdfs:subClassOf>
		<rdfs:subClassOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#Ul"></rdfs:subClassOf>
	</owl:Class>

	<owl:Class rdf:about="https://www.w3schools.com/tags/tag_ol.asp">
		<rdfs:label>Lista ordenada</rdfs:label>
		<rdfs:comment>Define una lista ordenada. Puede ser numérica o alfabética</rdfs:comment>
		<rdfs:subClassOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#Element"></rdfs:subClassOf>
		<rdfs:subClassOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#Nav"></rdfs:subClassOf>
	</owl:Class>

	<owl:Class rdf:about="https://www.w3schools.com/tags/tag_a.asp">
		<rdfs:label>Hiperenlace</rdfs:label>
		<rdfs:comment>Define un hiperenlace, el cual es usado para enlazar una página a otra</rdfs:comment>
		<rdfs:subClassOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#Element"></rdfs:subClassOf>
		<rdfs:subClassOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#Nav"></rdfs:subClassOf>
		<rdfs:subClassOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#Li"></rdfs:subClassOf>
		<rdfs:subClassOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#Ol"></rdfs:subClassOf>
	</owl:Class>

	<owl:Class rdf:about="https://www.w3schools.com/tags/tag_form.asp">
		<rdfs:label>Formulario</rdfs:label>
		<rdfs:comment>Es usado para crear un formulario HTML para una entrada de usuario</rdfs:comment>
		<rdfs:subClassOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#Element"></rdfs:subClassOf>
		<rdfs:subClassOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#Main"></rdfs:subClassOf>
	</owl:Class>

	<owl:Class rdf:about="https://www.w3schools.com/tags/tag_input.asp">
		<rdfs:label>Entrada</rdfs:label>
		<rdfs:comment>La etiqueta entrada especifica un campo de entrada donde el usuario puede ingresar información</rdfs:comment>
		<rdfs:subClassOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#Element"></rdfs:subClassOf>
		<rdfs:subClassOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#Main"></rdfs:subClassOf>
	</owl:Class>

	<owl:Class rdf:about="https://www.w3schools.com/tags/tag_main.asp">
		<rdfs:label>Sección principal</rdfs:label>
		<rdfs:comment>La etiqueta main especifica el contenido principal de un documento</rdfs:comment>
		<rdfs:subClassOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#Element"></rdfs:subClassOf>
	</owl:Class>

	<owl:Class rdf:about="https://www.w3schools.com/tags/tag_article.asp">
		<rdfs:label>Articulo</rdfs:label>
		<rdfs:comment>La etiqueta Articulo especifíca contenido independiente y autocontenido</rdfs:comment>
		<rdfs:subClassOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#Element"></rdfs:subClassOf>
		<rdfs:subClassOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#Main"></rdfs:subClassOf>
	</owl:Class>

	<owl:Class rdf:about="https://www.w3schools.com/tags/tag_aside.asp">
		<rdfs:label>Aparte</rdfs:label>
		<rdfs:comment>La etiqueta Aparte define un contenido aparte del contenido en que se encuentra</rdfs:comment>
		<rdfs:subClassOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#Element"></rdfs:subClassOf>
		<rdfs:subClassOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#Main"></rdfs:subClassOf>
	</owl:Class>

	<owl:Class rdf:about="https://www.w3schools.com/tags/tag_footer.asp">
		<rdfs:label>Pie de página</rdfs:label>
		<rdfs:comment>Define el pie de página de un documento o una sección</rdfs:comment>
		<rdfs:subClassOf rdf:resource="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#element"></rdfs:subClassOf>
	</owl:Class>

	<!--
		///////////////////////////////////////////////////////////////////////////////////////
		//
		// Individuals
		//
		///////////////////////////////////////////////////////////////////////////////////////
	-->
	<rdf:Description rdf:about="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#Header">
		<rdf:type rdf:resource="https://www.w3schools.com/tags/tag_header.asp"></rdf:type>
		<id rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">header</id>
		<role rdf:datatype="http://www.w3.org/2001/XMLSchema#string">banner</role>
	</rdf:Description>

	<rdf:Description rdf:about="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#Nav">
		<rdf:type rdf:resource="https://www.w3schools.com/tags/tag_nav.asp"></rdf:type>
		<id rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">nav</id>
		<role rdf:datatype="http://www.w3.org/2001/XMLSchema#string">navigation</role>
	</rdf:Description>

	<rdf:Description rdf:about="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#Ul">
		<rdf:type rdf:resource="https://www.w3schools.com/tags/tag_ul.asp"></rdf:type>
		<id rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">ul</id>
		<role rdf:datatype="http://www.w3.org/2001/XMLSchema#string">menubar</role>
		<aria_expanded rdf:datatype="http://www.w3.org/2001/XMLSchema#string">false</aria_expanded>
		<aria_hidden rdf:datatype="http://www.w3.org/2001/XMLSchema#string">false</aria_hidden>
	</rdf:Description>

	<rdf:Description rdf:about="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#Li">
		<rdf:type rdf:resource="https://www.w3schools.com/tags/tag_li.asp"></rdf:type>
		<id rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">li</id>
		<role rdf:datatype="http://www.w3.org/2001/XMLSchema#string">menuitem</role>
		<aria_haspopup rdf:datatype="http://www.w3.org/2001/XMLSchema#string">false</aria_haspopup>
		<aria_hidden rdf:datatype="http://www.w3.org/2001/XMLSchema#string">false</aria_hidden>
	</rdf:Description>

	<rdf:Description rdf:about="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#A">
		<rdf:type rdf:resource="https://www.w3schools.com/tags/tag_a.asp"></rdf:type>
		<id rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">a</id>
		<title rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></title>
		<href rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></href>
		<role rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></role>
		<rel rdf:datatype="http://www.w3.org/2001/XMLSchema#string">nofollow</rel>
	</rdf:Description>

	<rdf:Description rdf:about="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#Form">
		<rdf:type rdf:resource="https://www.w3schools.com/tags/tag_form.asp"></rdf:type>
		<id rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">form</id>
		<role rdf:datatype="http://www.w3.org/2001/XMLSchema#string">search</role>
		<aria_labelledby rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></aria_labelledby>
	</rdf:Description>

	<rdf:Description rdf:about="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#Input">
		<rdf:type rdf:resource="https://www.w3schools.com/tags/tag_input.asp"></rdf:type>
		<id rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">input</id>
		<type rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></type>
		<name rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></name>
		<placehoder rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></placehoder>
		<value rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></value>
	</rdf:Description>

	<rdf:Description rdf:about="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#Main">
		<rdf:type rdf:resource="https://www.w3schools.com/tags/tag_main.asp"></rdf:type>
		<id rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">main</id>
		<role rdf:datatype="http://www.w3.org/2001/XMLSchema#string">main</role>
	</rdf:Description>

	<rdf:Description rdf:about="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#Article">
		<rdf:type rdf:resource="https://www.w3schools.com/tags/tag_article.asp"></rdf:type>
		<id rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">article</id>
		<role rdf:datatype="http://www.w3.org/2001/XMLSchema#string">article</role>
	</rdf:Description>

	<rdf:Description rdf:about="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#Aside">
		<rdf:type rdf:resource="https://www.w3schools.com/tags/tag_article.asp"></rdf:type>
		<id rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">aside</id>
		<role rdf:datatype="http://www.w3.org/2001/XMLSchema#string">complementary</role>
	</rdf:Description>

	<rdf:Description rdf:about="http://www.semanticweb.org/juli/ontologies/2018/2/html-ontology#Footer">
		<rdf:type rdf:resource="https://www.w3schools.com/tags/tag_footer.asp"></rdf:type>
		<id rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">footer</id>
		<role rdf:datatype="http://www.w3.org/2001/XMLSchema#string">contentinfo</role>
	</rdf:Description>

</rdf:RDF>